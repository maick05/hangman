{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double"],
        "semi": ["error", "never"],
        "indent": ["error", "tab"],
        "no-tabs": 0,
        "space-before-function-paren":"off"
    }
}
