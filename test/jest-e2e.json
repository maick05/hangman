{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "roots": [
    "../"
  ],
  "modulePaths": [
    "../"
  ],
  "moduleDirectories": [
    "../node_modules",
    "../"
  ],
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t|j)s"
  ],
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
